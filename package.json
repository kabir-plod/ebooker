{
  "name": "ebooker",
  "version": "0.1.0",
  "description": "Browser extension to download web fiction as epubs",
  "scripts": {
    "dev": "webpack --config wdev.webpack.config.js --debug --output-pathinfo --progress --watch",
    "test": "webpack --config wtest.webpack.config.js --output-pathinfo --progress --watch",
    "start": "node server.js",
    "zuul": "zuul --local 8080 --ui mocha-qunit -- ./dist/test/test-script.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/kabir-plod/ebooker.git"
  },
  "author": "kabir-plod (http://github.com/kabir-plod)",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/kabir-plod/ebooker/issues"
  },
  "devDependencies": {
    "@types/chrome": "0.0.46",
    "@types/node": "^7.0.13",
    "@types/react": "^15.0.16",
    "@types/react-dom": "^0.14.23",
    "@types/tape": "^4.2.29",
    "awesome-typescript-loader": "^3.1.2",
    "copy-webpack-plugin": "^4.0.1",
    "tape": "^4.6.3",
    "typescript": "^2.2.1",
    "web-ext": "^1.9.0",
    "webpack": "^2.2.1",
    "webpack-dev-server": "^2.4.2",
    "zuul": "^3.11.1"
  },
  "dependencies": {
    "adm-zip": "^0.4.7",
    "mobx": "^3.1.7",
    "mobx-react": "^4.1.3",
    "react": "^15.4.2",
    "react-dom": "^15.4.2"
  }
